        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.uprr.training.pojos.Dates" table="DATES">
        <id name="id" column="ID"/>
        <property name="date" column="DATE"/>
    </class>
</hibernate-mapping>
